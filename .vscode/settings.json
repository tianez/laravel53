// 将设置放入此文件中以覆盖默认值和用户设置。
{

    "editor.lineHeight": 22,
    // 配置 glob 模式以排除文件和文件夹。
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/.DS_Store": true,
        "**/node_modules": true,
        ".gitattributes": true,
        ".env.example": true,
        "web.config": true,
        "robots.txt": true,
        "phpunit.xml": true,
        "artisan": true,
        "tests": true,
        ".htaccess": true,
        "composer.phar": true,
        "vendor.zip": true,
        "app/Exceptions":true,
        "app/Policies":true,
        "app/Jobs":true,
        "bootstrap":true
    }
}